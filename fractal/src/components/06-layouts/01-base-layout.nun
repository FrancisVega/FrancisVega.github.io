<!doctype html>
<html lang="es">

    <!--Head-->
    {% render '@html-head', {pageTitle: pageTitle, typekit: false, styles: ["main.css"]} %}

    <body>

        <!--Header-->
        {# render '@footer' #}

        <!--Content-->
        {%- block content %}
        <h3>Este Layout no se usa directamente</h3>
        <p>Extender con {% raw %}<span style="font-family:Inconsolata;color:blue;">{% extend '@section-layout' %}</span>{% endraw %}</p>
        {% endblock %}

        <!--Footer-->
        {# render '@footer' #}

        <!--Body Scripts-->
        {%- render '@body-scripts', { bundle: false } %}

    </body>

</html>
